@model List<MVC_HW_191130.Models.MoneyViewModel>


<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>@Html.DisplayNameFor(m=>m[0].類別)</th>
                <th>@Html.DisplayNameFor(m => m[0].時間)</th>
                <th>@Html.DisplayNameFor(m=>m[0].金錢)</th>
            </tr>
        @{
            for(var i = 0; i < Model.Count; i++)
            {
            <tr>
                <th>@(i+1)</th>
                <th>@Html.DisplayFor(m=>m[i].類別)</th>
                <th>@Html.DisplayFor(m => m[i].時間)</th>
                <th>@Html.DisplayFor(m => m[i].金錢)</th>
            </tr>
            }
        }
        </table>
    </div>
</div>
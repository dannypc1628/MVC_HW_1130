@model MVC_HW_191130.Models.GroupMoneyViewModel

@Html.Partial("CreateMoney")

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>@Html.DisplayNameFor(m=>m.ListMoney[0].類別)</th>
                <th>@Html.DisplayNameFor(m => m.ListMoney[0].時間)</th>
                <th>@Html.DisplayNameFor(m=>m.ListMoney[0].金錢)</th>
            </tr>
        @{
            for(var i = 0; i < Model.ListMoney.Count; i++)
            {
            <tr>
                <th>@(i+1)</th>
                <th>@Html.DisplayFor(m=>m.ListMoney[i].類別)</th>
                <th>@Html.DisplayFor(m => m.ListMoney[i].時間)</th>
                <th>@Html.DisplayFor(m => m.ListMoney[i].金錢)</th>
            </tr>
            }
        }
        </table>
    </div>
</div>